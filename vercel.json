{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/riot.txt",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "redirects": [],
  "rewrites": [
    {
      "source": "/riot.txt",
      "destination": "/riot.txt"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}